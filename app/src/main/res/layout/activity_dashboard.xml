                            android:layout_width="0dp"
                            android:layout_height="32dp"
                            android:layout_marginLeft="4dp"
                            android:layout_weight="1"
                            android:background="@drawable/round_button_background"
                            android:text="Daugiau"
                            android:textAllCaps="false"
                            android:textColor="@android:color/black" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linear_layout_image_text"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="4dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginRight="4dp"
                        android:orientation="horizontal"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/linear_layout_app_stats">

                        <ImageView
                            android:id="@+id/image1"
                            android:layout_width="0dp"
                            android:layout_height="38dp"
                            android:layout_weight="1"
                            android:src="@drawable/round_fragment_corners" />

                        <TextView
                            android:id="@+id/text1"
                            android:layout_width="0dp"
                            android:layout_height="38dp"
                            android:gravity="center"
                            android:text=""
                            android:textColor="@android:color/white"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            android:layout_weight="5 " />

                        <TextView
                            android:id="@+id/text2"
                            android:layout_width="0dp"
                            android:layout_height="38dp"
                            android:gravity="center"
                            android:text=""
                            android:textColor="@android:color/white"
                            android:textSize="16sp"
                            android:layout_weight="2.5"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linear_layout_image_text2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="4dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginRight="4dp"
                        android:orientation="horizontal"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/linear_layout_image_text">

                    <ImageView
                        android:id="@+id/image2"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:layout_weight="1"
                        android:src="@drawable/round_fragment_corners"/>

                    <TextView
                        android:id="@+id/text3"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@android:color/white"
                        android:textSize="16sp"
                        android:layout_weight="5"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/text4"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@android:color/white"
                        android:textSize="16sp"
                        android:layout_weight="2.5" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linear_layout_image_text3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="4dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginRight="4dp"
                        android:orientation="horizontal"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/linear_layout_image_text2">

                    <ImageView
                        android:id="@+id/image3"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:layout_weight="1"
                        android:src="@drawable/round_fragment_corners"/>

                    <TextView
                        android:id="@+id/text5"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@android:color/white"
                        android:textSize="16sp"
                        android:layout_weight="5"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/text6"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:gravity="center"
                        android:text=""
                        android:layout_weight="2.5"
                        android:textColor="@android:color/white"
                        android:textSize="16sp"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linear_layout_image_text4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="4dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginRight="4dp"
                        android:orientation="horizontal"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/linear_layout_image_text3">

                    <ImageView
                        android:id="@+id/image4"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:layout_weight="1"
                        android:src="@drawable/round_fragment_corners"/>

                    <TextView
                        android:id="@+id/text7"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@android:color/white"
                        android:textSize="16sp"
                        android:textStyle="bold"
                        android:layout_weight="5" />

                    <TextView
                        android:id="@+id/text8"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@android:color/white"
                        android:textSize="16sp"
                        android:layout_weight="2.5" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linear_layout_image_text5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="4dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginRight="4dp"
                        android:orientation="horizontal"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/linear_layout_image_text4">

                    <ImageView
                        android:id="@+id/image5"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:layout_weight="1"
                        android:src="@drawable/round_fragment_corners" />

                    <TextView
                        android:id="@+id/text9"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@android:color/white"
                        android:textSize="16sp"
                        android:textStyle="bold"
                        android:layout_weight="5" />

                    <TextView
                        android:id="@+id/text10"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@android:color/white"
                        android:textSize="16sp"
                        android:layout_weight="2.5" />

                    </LinearLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>

            </FrameLayout>

            <FrameLayout
                android:id="@+id/appUnlockStats"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="27dp"
                android:layout_marginTop="22dp"
                android:layout_marginRight="27dp"
                android:layout_marginBottom="16dp"
                android:background="@drawable/round_fragment_corners"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/appTimeStats">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="16dp">

                    <LinearLayout
                        android:id="@+id/linear_layout_app_stats2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="4dp"
                        android:layout_marginTop="4dp"
                        android:layout_marginRight="4dp"
                        android:orientation="horizontal"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                    <TextView
                        android:id="@+id/textAppUnlockStats"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="3"
                        android:text="Dažniausiai atidaromos"
                        android:textColor="@android:color/white"
                        android:textSize="20sp"
                        android:textStyle="bold"  />

                    <Button
                        android:id="@+id/mostOpenedButton"
                        android:layout_width="0dp"
                        android:layout_height="32dp"
                        android:background="@drawable/round_button_background"
                        android:text="Daugiau"
                        android:textAllCaps="false"
                        android:layout_weight="1"
                        android:textColor="@android:color/black" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linear_layout_image_text6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="4dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginRight="4dp"
                        android:orientation="horizontal"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/linear_layout_app_stats2">

                    <ImageView
                        android:id="@+id/image6"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:src="@drawable/round_fragment_corners"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/text11"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:layout_weight="5"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@android:color/white"
                        android:textSize="16sp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/text12"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:layout_weight="2.5"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@android:color/white"
                        android:textSize="16sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linear_layout_image_text7"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="4dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginRight="4dp"
                        android:orientation="horizontal"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/linear_layout_image_text6">

                    <ImageView
                        android:id="@+id/image7"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/text13"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:layout_weight="5"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@android:color/white"
                        android:textSize="16sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/text14"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:layout_weight="2.5"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@android:color/white"
                        android:textSize="16sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linear_layout_image_text8"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="4dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginRight="4dp"
                        android:orientation="horizontal"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/linear_layout_image_text7">

                    <ImageView
                        android:id="@+id/image8"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:src="@drawable/round_fragment_corners"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/text15"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:layout_weight="5"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@android:color/white"
                        android:textSize="16sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/text16"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:layout_weight="2.5"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@android:color/white"
                        android:textSize="16sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linear_layout_image_text9"
